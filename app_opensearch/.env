SOFTWARE_VERSION_TAG=latest
POSTGRES_DB=etllogstash
POSTGRES_USER=etldor
POSTGRES_PASSWORD=etlPassWord
PGDATA=/var/lib/postgresql/data

#Opensearch config

OPENSEARCH_INITIAL_ADMIN_PASSWORD=q^wER4_@!
OPENSEARCH_JAVA_OPTS=-Xms512m -Xmx512m  # minimum and maximum Java heap size, recommend setting both to 50% of system RAM
cluster.name=opensearch-cluster
node.name=opensearch-etllogstash-node1
discovery.seed_hosts=opensearch-etllogstash-node1,opensearch-node2
cluster.initial_cluster_manager_nodes=opensearch-etllogstash-node1,opensearch-node2
bootstrap.memory_lock=true  # along with the memlock settings below, disables swapping

discovery.seed_hosts: [ 'node-1', 'node-2', 'node-3' ]
cluster.initial_master_nodes: [ 'node-1', 'node-2', 'node-3' ]